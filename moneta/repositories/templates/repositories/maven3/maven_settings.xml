{% load moneta  %}<repository>
    <id>snapshots</id>
    {% with view=repo|moneta_url:'browse' %}{% if state_slug %}<url>{{ absolute_url }}{% url view rid=repo.id repo_slug=repo.slug state_slug=state_slug %}</url>
    {% else %}<url>{{ absolute_url }}{% url view rid=repo.id repo_slug=repo.slug %}</url>
    {% endif %}<releases>
        <enabled>false</enabled>
    </releases>{% endwith %}
</repository>
